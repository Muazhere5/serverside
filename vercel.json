{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/featured-habits",
      "dest": "index.js"
    },
    {
      "src": "/my-habits/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/add-habit",
      "dest": "index.js"
    },
    {
      "src": "/habit/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}